		"tailwindcss": "^3.3.1",
		"marked": "^5.1.2",
		"three": "^0.152.2",
		"@tanstack/svelte-query": "^4.24.4",
		"aos": "^2.3.4",
		"gsap": "^3.12.5"
	}
